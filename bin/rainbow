#!/bin/bash
#
# rainbow
#
# All 16 colors of the rainbow
#
# cutejs (Astrid Fesz-Nguyen IV)
# 2019-08-21

printblocks () {
  echo $'@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@'
  echo -en '\e[0m'
}

echo -en '\e[0m'
# black
echo -en '\e[30m'
printblocks
# black bold
echo -en '\e[1;30m'
printblocks
# red
echo -en '\e[0;31m'
printblocks
# red bold
echo -en '\e[1;31m'
printblocks
# green
echo -en '\e[32m'
printblocks
# green bold
echo -en '\e[1;32m'
printblocks
# yellow 
echo -en '\e[33m'
printblocks
# yellow bold
echo -en '\e[1;33m'
printblocks
# blue
echo -en '\e[34m'
printblocks
# blue bold
echo -en '\e[1;34m'
printblocks
# uhhh
echo -en '\e[35m'
printblocks

echo -en '\e[1;35m'
printblocks

echo -en '\e[36m'
printblocks

echo -en '\e[1;36m'
printblocks

echo -en '\e[37m'
printblocks

echo -en '\e[1;37m'
printblocks
